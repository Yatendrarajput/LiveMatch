{
    "manifest_version": 3,
    "name": "LiveMatch",
    "version": "1.0",
    "author":"Yatendra Rajput",
    "description": "Chrome extension for live cricket match updates.",
    "icons": {
      "16": "icons/16.png",
      "128": "icons/128.png"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icons/16.png",
        "128": "icons/128.png"
      }
    },
    "permissions": [
      "storage",
      "tabs"
    ],
    "web_accessible_resources": [
      {
        "resources": ["icons/*"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  